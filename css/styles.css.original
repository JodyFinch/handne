/* Privacy Modal Styling */
#privacyModal .modal-content {
  border-radius: 10px;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
}

#privacyModal .modal-header {
  background-color: var(--color-primary-hover);
  color: var(--color-text-white);
  border-radius: 10px 10px 0 0;
  padding: 20px;
}

#privacyModal .modal-header .close {
  color: var(--color-text-white);
  opacity: 0.8;
  font-size: var(--font-size-xxl);
}

#privacyModal .modal-header .close:hover {
  opacity: 1;
}

#privacyModal .modal-title {
  font-weight: 600;
  font-size: var(--font-size-xxl);
}

#privacyModal .modal-body {
  padding: 30px;
  line-height: 1.6;
  font-size: 0.95rem;
}

#privacyModal .modal-body h6 {
  color: var(--color-primary-hover);
  font-weight: 600;
  margin-top: 25px;
  margin-bottom: 15px;
  font-size: 1.1rem;
}

#privacyModal .modal-body h6:first-of-type {
  margin-top: 0;
}

#privacyModal .modal-body ul {
  padding-left: 20px;
}

#privacyModal .modal-body li {
  margin-bottom: 8px;
}

#privacyModal .modal-body strong {
  color: var(--color-primary-hover);
}

#privacyModal .modal-body a {
  color: var(--color-primary-hover);
  text-decoration: underline;
}

#privacyModal .modal-body a:hover {
  color: #0d3d7a;
}

#privacyModal .modal-footer {
  border-top: 1px solid #e9ecef;
  padding: 20px 30px;
}

#privacyModal .modal-footer .btn {
  border-radius: 5px;
  padding: 10px 20px;
  font-weight: 500;
}

/* Privacy button styling in footer */
.privacy-notice-btn {
  border: 2px solid var(--color-text-white) !important;
  background-color: transparent !important;
  color: var(--color-text-white) !important;
  font-weight: 600 !important;
  font-size: 0.9rem !important;
  padding: 8px 16px !important;
  border-radius: 25px !important;
  transition: all var(--transition-fast) !important;
  text-decoration: none !important;
  box-shadow: 0 2px 8px rgba(26, 86, 169, 0.2) !important;
}

.privacy-notice-btn:hover {
  background-color: var(--color-primary-hover) !important;
  color: var(--color-text-white) !important;
  border-color: var(--color-primary-hover) !important;
  transform: translateY(-2px) !important;
  box-shadow: 0 4px 15px rgba(26, 86, 169, 0.4) !important;
}

.privacy-notice-btn:focus {
  box-shadow: 0 0 0 3px rgba(26, 86, 169, 0.3) !important;
  outline: none !important;
}

.privacy-notice-btn:active {
  transform: translateY(0) !important;
  box-shadow: 0 2px 8px rgba(26, 86, 169, 0.3) !important;
}

